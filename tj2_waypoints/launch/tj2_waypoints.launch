<launch>
    <arg name="waypoints_path" default="waypoints.yaml"/>
    <group ns="tj2" >
        <node name="tj2_waypoints" pkg="tj2_waypoints" type="tj2_waypoints_node.py" output="log" required="false">
            <param name="waypoints_path" value="$(arg waypoints_path)"/>
            <param name="intermediate_tolerance" value="0.0"/>
            <param name="enable_waypoint_navigation" value="true"/>
        </node>
    </group>
</launch>
